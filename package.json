    "storybook": "storybook dev -p 6006",
    "build-storybook": "nx exec -- storybook build",
    "_build": "tsc && vite build",
    "test:storybook": "eyes-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
